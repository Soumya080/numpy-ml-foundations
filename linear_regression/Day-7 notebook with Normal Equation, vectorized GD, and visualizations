      "source": [
        "# Day-7: Refactor of your multivariate regression code + visuals + Normal Equation reference\n",
        "import numpy as np\n",
        "import matplotlib.pyplot as plt\n",
        "\n",
        "np.random.seed(0)\n",
        "\n",
        "# 1) Data generation (your original)\n",
        "\n",
        "x1_data = np.random.rand(100)\n",
        "x2_data = np.random.rand(100)\n",
        "y_data = x1_data*0.1 + x2_data*2.5 + np.random.randn(100)\n",
        "\n",
        "# split the data\n",
        "split_idx = int(0.8 * len(x1_data))\n",
        "x1_train = x1_data[:split_idx]\n",
        "x2_train = x2_data[:split_idx]\n",
        "y_train = y_data[:split_idx].reshape(-1, 1)\n",
        "x1_test = x1_data[split_idx:]\n",
        "x2_test = x2_data[split_idx:]\n",
        "y_test = y_data[split_idx:].reshape(-1, 1)\n",
        "\n",
        "# Combine features (your original X_train / X_test)\n",
        "X_train = np.column_stack((x1_train, x2_train))\n",
        "X_test  = np.column_stack((x1_test,  x2_test))\n",
        "\n",
        "\n",
        "# 2) Scaling\n",
        "\n",
        "x_mean = np.mean(X_train, axis=0)\n",
        "x_std  = np.std(X_train, axis=0)\n",
        "X_train_scaled = (X_train - x_mean) / x_std\n",
        "X_test_scaled  = (X_test - x_mean) / x_std\n",
        "\n",
        "print(\"X_train_scaled shape:\", X_train_scaled.shape)\n",
        "print(\"X_test_scaled shape: \", X_test_scaled.shape)\n",
        "\n",
        "# 3) Utility / refactored functions (replaces your modular functions)\n",
        "\n",
        "def build_design_matrix(X):\n",
        "    \"\"\"Return X_design with bias column. X shape (m, n). Result (m, n+1).\"\"\"\n",
        "    X = np.asarray(X)\n",
        "    if X.ndim == 1:\n",
        "        X = X.reshape(-1, 1)\n",
        "    ones = np.ones((X.shape[0], 1))\n",
        "    return np.hstack([X, ones])  # (m, n+1)\n",
        "\n",
        "def initialize_parameters(n_features, rand_seed=None):\n",
        "    \"\"\"Initialize theta = [w; b] of shape (n_features+1, 1).\"\"\"\n",
        "    if rand_seed is not None:\n",
        "        np.random.seed(rand_seed)\n",
        "    theta = np.random.randn(n_features + 1, 1) * 0.1\n",
        "    return theta\n",
        "\n",
        "def predict_design(X_design, theta):\n",
        "    \"\"\"Predict using design matrix (m, n+1) and theta (n+1,1).\"\"\"\n",
        "    return X_design @ theta\n",
        "\n",
        "def compute_loss(y_true, y_pred):\n",
        "    \"\"\"Mean squared error (scalar).\"\"\"\n",
        "    return float(np.mean((y_true - y_pred)**2))\n",
        "\n",
        "def compute_gradients_design(X_design, y, theta):\n",
        "    \"\"\"\n",
        "    Vectorized gradients for MSE w.r.t theta.\n",
        "    X_design: (m, n+1), theta: (n+1,1), y: (m,1)\n",
        "    returns grad (n+1,1)\n",
        "    \"\"\"\n",
        "    m = X_design.shape[0]\n",
        "    y_pred = predict_design(X_design, theta)\n",
        "    err = y_pred - y                               grad = (2.0 / m) * (X_design.T @ err)\n",
        "    return grad\n",
        "\n",
        "def update_theta(theta, grad, lr):\n",
        "    return theta - lr * grad\n",
        "\n",
        "def gradient_descent_design(X_design, y, theta_init=None, lr=0.01, epochs=1000, tol=1e-12, verbose=False):\n",
        "    \"\"\"Batch GD optimizing theta. Returns theta_final and history dict.\"\"\"\n",
        "    m, n1 = X_design.shape\n",
        "    if theta_init is None:\n",
        "        theta = initialize_parameters(n1 - 1)\n",
        "    else:\n",
        "        theta = theta_init.reshape(n1, 1)\n",
        "    loss_history = []\n",
        "    theta_history = []\n",
        "    for epoch in range(epochs):\n",
        "        y_pred = predict_design(X_design, theta)\n",
        "        loss = compute_loss(y, y_pred)\n",
        "        loss_history.append(loss)\n",
        "        theta_history.append(theta.flatten().copy())\n",
        "        grad = compute_gradients_design(X_design, y, theta)\n",
        "        theta = update_theta(theta, grad, lr)\n",
        "        if epoch > 0 and abs(loss_history[-2] - loss_history[-1]) < tol:\n",
        "            if verbose:\n",
        "                print(f\"Early stop at epoch {epoch}, loss change {abs(loss_history[-2] - loss_history[-1])}\")\n",
        "            break\n",
        "    return theta, {\"loss_history\": loss_history, \"theta_history\": np.array(theta_history)}\n",
        "\n",
        "def normal_equation(X_design, y, ridge_eps=1e-8):\n",
        "    \"\"\"\n",
        "    Closed-form solution using pseudoinverse for stability.\n",
        "    theta = (X^T X + eps I)^{-1} X^T y  (equivalently use np.linalg.pinv)\n",
        "    \"\"\"\n",
        "    XTX = X_design.T @ X_design\n",
        "    XTX_reg = XTX + ridge_eps * np.eye(XTX.shape[0])\n",
        "    theta = np.linalg.inv(XTX_reg) @ (X_design.T @ y)\n",
        "    return theta.reshape(-1, 1)\n",
        "\n",
        "\n",
        "# 4) Build design matrices and initial params\n",
        "-\n",
        "Xd_train = build_design_matrix(X_train_scaled)   # (m_train, 3)\n",
        "Xd_test  = build_design_matrix(X_test_scaled)    # (m_test, 3)\n",
        "\n",
        "# initialize theta similar to your earlier initiallise_parameters (kept small random)\n",
        "theta0 = initialize_parameters(n_features = X_train_scaled.shape[1], rand_seed=1)\n",
        "\n",
        "\n",
        "# 5) Normal Equation solution\n",
        "\n",
        "theta_ne = normal_equation(Xd_train, y_train, ridge_eps=1e-10)\n",
        "w_ne = theta_ne[:-1].ravel()\n",
        "b_ne = float(theta_ne[-1])\n",
        "print(\"Normal Eq theta (w1,w2,b):\", theta_ne.ravel())\n",
        "\n",
        "# Orthogonality check: X^T (y - X_ne)  0\n",
        "orth_check = Xd_train.T @ (y_train - predict_design(Xd_train, theta_ne))\n",
        "print(\"Orthogonality check (should be near zero):\", orth_check.ravel())\n",
        "\n",
        "# 6) Gradient Descent training (vectorized)\n",
        "\n",
        "lr = 0.01\n",
        "epochs = 2000\n",
        "theta_gd, history = gradient_descent_design(Xd_train, y_train, theta_init=theta0, lr=lr, epochs=epochs, tol=1e-12, verbose=False)\n",
        "print(\"GD theta (w1,w2,b):\", theta_gd.ravel())\n",
        "\n",
        "# compute predictions and losses\n",
        "y_train_pred = predict_design(Xd_train, theta_gd)\n",
        "y_test_pred  = predict_design(Xd_test, theta_gd)\n",
        "train_loss = compute_loss(y_train, y_train_pred)\n",
        "test_loss  = compute_loss(y_test,  y_test_pred)\n",
        "\n",
        "print(f\"Train MSE (GD): {train_loss:.6f}\")\n",
        "print(f\"Test  MSE (GD): {test_loss:.6f}\")\n",
        "print(f\"Train MSE (Normal Eq): {compute_loss(y_train, predict_design(Xd_train, theta_ne)):.6f}\")\n",
        "\n",
        "# Compare NE vs GD predictions norm difference\n",
        "pred_diff_norm = np.linalg.norm(predict_design(Xd_train, theta_ne) - y_train_pred)\n",
        "print(\"||y_pred_ne - y_pred_gd|| (train):\", pred_diff_norm)\n",
        "\n",
        "\n",
        "# 7) Derived metrics: MAE for train/test (as you computed)\n",
        "\n",
        "train_mae = np.mean(np.abs(y_train - y_train_pred))\n",
        "test_mae  = np.mean(np.abs(y_test  - y_test_pred))\n",
        "print(\"Train MAE:\", train_mae)\n",
        "print(\"Test  MAE:\", test_mae)\n",
        "\n",
        "\n",
        "# 8) Visualizations\n",
        "\n",
        "# a) Loss history\n",
        "plt.figure(figsize=(6,3))\n",
        "plt.plot(history[\"loss_history\"], label='GD Loss')\n",
        "plt.yscale('log')\n",
        "plt.xlabel('Epoch'); plt.ylabel('MSE'); plt.title('Training Loss (log scale)')\n",
        "plt.grid(True); plt.legend()\n",
        "plt.show()\n",
        "\n",
        "# b) True vs Predicted (train and test)\n",
        "plt.figure(figsize=(12,5))\n",
        "plt.subplot(1,2,1)\n",
        "plt.scatter(y_train, y_train_pred, s=20, alpha=0.6)\n",
        "plt.plot([y_train.min(), y_train.max()], [y_train.min(), y_train.max()], 'r--')\n",
        "plt.xlabel('y_true (train)'); plt.ylabel('y_pred (train)'); plt.title('Train: True vs Pred')\n",
        "plt.grid(True)\n",
        "\n",
        "plt.subplot(1,2,2)\n",
        "plt.scatter(y_test, y_test_pred, s=20, alpha=0.6, color='orange')\n",
        "plt.plot([y_test.min(), y_test.max()], [y_test.min(), y_test.max()], 'r--')\n",
        "plt.xlabel('y_true (test)'); plt.ylabel('y_pred (test)'); plt.title('Test: True vs Pred')\n",
        "plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "# c) Residual histograms (train)\n",
        "res_train = (y_train - y_train_pred).ravel()\n",
        "plt.figure(figsize=(6,3))\n",
        "plt.hist(res_train, bins=25, alpha=0.7)\n",
        "plt.title('Train residuals distribution')\n",
        "plt.xlabel('Residual'); plt.ylabel('Count'); plt.grid(True)\n",
        "plt.show()\n",
        "\n",
        "# d) Loss surface over (w1,w2) keeping b fixed at NE b (visual intuition)\n",
        "\n",
        "w1_center, w2_center = w_ne[0], w_ne[1]\n",
        "w1_vals = np.linspace(w1_center - 1.0, w1_center + 1.0, 80)\n",
        "w2_vals = np.linspace(w2_center - 1.0, w2_center + 1.0, 80)\n",
        "W1, W2 = np.meshgrid(w1_vals, w2_vals)\n",
        "Loss_surf = np.zeros_like(W1)\n",
        "for i in range(W1.shape[0]):\n",
        "    for j in range(W1.shape[1]):\n",
        "        theta_grid = np.array([[W1[i,j]], [W2[i,j]], [b_ne]])  # b fixed at NE bias\n",
        "        preds = Xd_train @ theta_grid\n",
        "        Loss_surf[i,j] = np.mean((preds - y_train)**2)\n",
        "\n",
        "plt.figure(figsize=(7,5))\n",
        "cont = plt.contourf(W1, W2, Loss_surf, levels=40)\n",
        "plt.colorbar(cont, label='MSE loss')\n",
        "# Plot GD path projected to (w1,w2)\n",
        "theta_hist = history[\"theta_history\"]\n",
        "plt.plot(theta_hist[:,0], theta_hist[:,1], color='white', marker='o', markersize=3, linewidth=1, label='GD path')\n",
        "plt.scatter([w_ne[0]], [w_ne[1]], color='red', s=80, marker='*', label='Normal Eq (NE)')\n",
        "plt.xlabel('w1'); plt.ylabel('w2'); plt.title('Loss surface (b fixed) with GD path')\n",
        "plt.legend(); plt.show()\n",
        "\n",
        "# e) Print final parameters for clarity\n",
        "print(\"Final (NE): w =\", w_ne, \", b =\", b_ne)\n",
        "print(\"Final (GD): w =\", theta_gd[:-1].ravel(), \", b =\", float(theta_gd[-1]))\n"
      ]
    },
